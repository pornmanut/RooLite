{
  "defaults": {
    "timeout": 30000,
    "retries": 3,
    "cacheTimeout": 3600,
    "autostart": false
  },
  "serverDefaults": {
    "env": {
      "variables": {},
      "secrets": {}
    },
    "auth": {
      "type": "none",
      "credentials": {}
    },
    "settings": {
      "timeout": 30000,
      "retries": 3,
      "cacheTimeout": 3600
    }
  },
  "templates": {
    "apiKey": {
      "auth": {
        "type": "api_key",
        "credentials": {
          "apiKey": ""
        }
      }
    },
    "oauth": {
      "auth": {
        "type": "oauth",
        "credentials": {
          "clientId": "",
          "clientSecret": "",
          "refreshToken": ""
        }
      }
    }
  },
  "validation": {
    "envNamePattern": "^[A-Z][A-Z0-9_]*$",
    "commandPattern": "^[a-zA-Z0-9\\-_.\\/]+$",
    "timeoutRange": {
      "min": 1000,
      "max": 300000
    },
    "retriesRange": {
      "min": 0,
      "max": 10
    }
  }
}